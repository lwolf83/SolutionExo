using System;
using System.Collections.Generic;

namespace ConsoleApp13
{
    class Program
    {
        static void Main(string[] args)
        {
            Dictionary<char, string> dictionary = new Dictionary<char, string>();
            dictionary['a'] = "avion";
            dictionary['b'] = "bateau";
            dictionary['c'] = "camion";

            dictionary.Remove('c');
        }
    }
}
